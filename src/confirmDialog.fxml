        <buttons>
          <Button fx:id="btnYes" mnemonicParsing="false" onAction="#onBtnYes" text="Yes" />
            <Button fx:id="btnNo" mnemonicParsing="false" onAction="#onBtnNo" text="No" />
        </buttons>
      </ButtonBar>
